class Box
{
	double length,width,height;
	void area(double length,double width,double height)
{
	this.length=length;
	this.width=width;
	this.height=height;
}
	void display()
{
	System.out.println("Area : "+(length*width*height))
}
}
class Demo
{
	public static void main(String[]args)
	{
		Box b=new Box();
		b.area(1,1,1);
		b.display();
	}
}
